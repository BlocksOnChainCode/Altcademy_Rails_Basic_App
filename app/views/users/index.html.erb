# render a list of all user
<%# Compare this snippet from app/views/users/index.html.erb: %>
  <h1>Users</h1>
<% @users.each do |user| %>
    <p>Username: <%= user.username %></p>
    <p>Email: <%= user.email %></p>
    <p>First Name: <%= user.first_name %></p>
    <p>Last Name: <%= user.last_name %></p>
<% end %>